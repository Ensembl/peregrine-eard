program "group" "program" 1;

world code wc(?X) -> ?X { impl(r1: ?X) -> r2: ?X { opcode 21, r2 ,r1; } }
function c(x) { let y = wc(x); y }

print(format(c("hello, world!")));
print(format(c([1,2,3])));
print(format(c(["hi",...])));

