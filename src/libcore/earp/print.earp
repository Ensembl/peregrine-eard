code __code_format(?X) -> string {
    impl (r1: ?X) -> r2: string {
        opcode 9, r2, r1;
    }
}

export function format(x: ?X) -> string { let y = __code_format(x); y }

world code __code_print(string) {
    impl (r1: string) {
        opcode 8, r1;
    }
}

export procedure print(x: string) { __code_print(x); }
